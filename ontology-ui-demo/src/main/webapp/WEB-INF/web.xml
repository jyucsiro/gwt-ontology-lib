<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Servlets -->

	<servlet>
		<servlet-name>ontologyQueryServiceImpl</servlet-name>
		<servlet-class>au.csiro.eis.ontology.gwt.rpc.server.OntologyQueryServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ontologyQueryServiceImpl</servlet-name>
		<url-pattern>/ExampleUi/ontologyQuery</url-pattern>
	</servlet-mapping>
	
<servlet>
  <servlet-name>jUnitHostImpl</servlet-name>
  <servlet-class>com.google.gwt.junit.server.JUnitHostImpl</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>jUnitHostImpl</servlet-name>
  <url-pattern>/ExampleUi/junithost/*</url-pattern>
</servlet-mapping>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>example-ui.html</welcome-file>
  </welcome-file-list>

</web-app>
